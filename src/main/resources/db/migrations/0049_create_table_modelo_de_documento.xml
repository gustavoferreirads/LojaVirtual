    <changeSet author="laas" id="49">

        <createTable tableName="MODELO_DE_DOCUMENTO">
            <column name="ID_MODELO_DE_DOCUMENTO" type="BIGINT" autoIncrement="true" >
                <constraints primaryKey="true" />
            </column>
            <column name="TX_DESCRICAO_DOCUMENTO" type="VARCHAR(100)" />
            <column name="TX_DOCUMENTO" type="TEXT" />
            <column name="TS_ULT_MODIFICACAO" type="${TIMESTAMP}" />
            <column name="TS_DATA_CADASTRO" type="${TIMESTAMP}" />
            <column name="ID_TIPO_CADASTRO" type="BIGINT" />
