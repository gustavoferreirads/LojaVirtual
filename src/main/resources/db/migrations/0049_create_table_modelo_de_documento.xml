        </createTable>


        <addForeignKeyConstraint baseTableName="MODELO_DE_DOCUMENTO" baseColumnNames="ID_TIPO_CADASTRO" constraintName="FK_MODELO_DE_DOCUMENTO_ID_TIPO_CADASTRO"
                                 referencedTableName="TIPO_CADASTRO" referencedColumnNames="ID_TIPO_CADASTRO" />

        <addForeignKeyConstraint baseTableName="MODELO_DE_DOCUMENTO" baseColumnNames="ID_TIPO_CADASTRO" constraintName="FK_MODELO_DE_DOCUMENTO_ID_CLIENTE_LAAS"
